{
  "name": "@skribble/worker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "bullmq": "5.10.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@types/node": "20.14.10",
    "eslint": "9.8.0",
    "typescript": "5.5.4",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "vitest": "1.6.0"
  }
}
